<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">age</th>
      <th scope="col">Delete User</th>
    </tr>
  </thead>
  <tbody>
    <%@users.map do |user|%>
    <tr>
      <th scope="row">1</th>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.age %></td>
      <td> <%= link_to  "Delete", admin_user_path(user), data:{
        turbo_method: "delete",
        turbo_confirm: "Are you sure"
      }%> </td>
    </tr>
    <%end%>
  </tbody>
</table>

